{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "none",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "off",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder":
    [
      "^@/app/(.*)$",
      "^[./]",
      "^@/configs/(.*)$",
      "^@/api/(.*)$",
      "^@/types/(.*)$",
      "^@/states/(.*)$",
      "^@/model/(.*)$",
      "^@/utils/(.*)$",
      "^@/hooks/(.*)$",
      "^@/styles/(.*)$",
      "^@/public/(.*)$",
      "^@/constants/(.*)$",
      "^@/assets/(.*)$",
      "^@/components/(.*)$"
    ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
